def subArrayExists(arr,n):
    n = len(arr)
    for i in range(n): 
        curr_sum = arr[i]
        if curr_sum == 0:
            return True
        if curr_sum == 0:
            return True
        j = i + 1
        while j <= n: 
            if curr_sum == 0:
                # print ("Sum found between") 
                # print("indexes % d and % d"%( i, j-1)) 
                return True
            if j == n:
                break
            curr_sum = curr_sum + arr[j] 
            j += 1
    # print ("No subarray found")
    return False
